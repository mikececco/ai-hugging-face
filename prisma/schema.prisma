generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // or your database provider
  url      = env("DATABASE_URL")
  connectionTimeout = 30
}

model Business {
  id               Int      @id @default(autoincrement())
  name             String
  description      String
  price            Int
  employees        Int
  yearEstablished  Int
  location         String
  createdAt        DateTime @default(now())
}
